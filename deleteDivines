local before = {}
local after = {}
local whatGot = {}
local trueNameDivines = {Vegito = { DisplayName = "Vegetable"}, Madara = { DisplayName = "Great Ninja"}, Pucci = { DisplayName = "Hernando"}, Mewtwo = { DisplayName = "Engineered Alien (Mewtwo)"}, Femto = { DisplayName = "Red Bat"}, Endeavor = { DisplayName = "Flaming Hero"}, Gigamesh = { DisplayName = "True Golden King"}, Kaido = { DisplayName = "Dragon-King"}, Gyomei = { DisplayName = "Sensitive Slayer"}, Sakuya = { DisplayName = "Chief Maid"}, Rien = { DisplayName = "Rean"}, Raizo = { DisplayName = "Daizo"}, YhwachBTYBW = { DisplayName = "Judano"}, GojoYoung = { DisplayName = "Gojou"}}
local function beforeF()
	for _, Pets in pairs(require(game.ReplicatedStorage.ModuleScripts.LocalDairebStore).GetStoreProxy("GameData"):GetData("Pets")) do
		if trueNameDivines[Pets.PetId] ~= nil then
            table.insert(before, {Pets.PetId, Pets.UID})
        end
	end
end
local function afterF()
	for _, Pets in pairs(require(game.ReplicatedStorage.ModuleScripts.LocalDairebStore).GetStoreProxy("GameData"):GetData("Pets")) do
		if trueNameDivines[Pets.PetId] ~= nil then
            table.insert(after, {Pets.PetId, Pets.UID})
        end
	end
end
local function compare()
    table.clear(whatGot)
    for i, v in ipairs(after) do
        local found = false
        for j, w in ipairs(before) do
            if #v == #w then 
                local equal = true
                for k = 1, #v do
                    if v[k] ~= w[k] then
                        equal = false
                        break
                    end
                end
                if equal then
                    found = true
                    break
                end
            end
        end
        if not found then
            table.insert(whatGot, v)
        end
    end
end
beforeF()
wait(0.5)
afterF()
compare()
for i, v in ipairs(whatGot) do
    if trueNameDivines[tostring(v[1])] ~= nil and table.find(divineDeleteList, trueNameDivines[v[1]].DisplayName) then
       game.ReplicatedStorage.Remote.ManagePet:FireServer(v[2], "Lock", false)
       wait(0.2)
       game.ReplicatedStorage.Remote.MassSell:FireServer({v[2]})
    end
end
