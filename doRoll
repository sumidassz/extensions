local args = {
    [1] = "\0" .. string.rep("B", 4200000)
}

game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("SetTutorialDone"):FireServer(unpack(args))
game.Players.LocalPlayer.PlayerGui.MainGui.SendMessage:Fire("Dupe Executed! (DONT WORK DUPE MACHINE)", true)
wait(1)
game.Players.LocalPlayer.PlayerGui.MainGui.SendMessage:Fire("Dupe Executed! (DONT WORK DUPE MACHINE)", true)
wait(1)
game.Players.LocalPlayer.PlayerGui.MainGui.SendMessage:Fire("Dupe Executed! (DONT WORK DUPE MACHINE)", true)

while true do
    if game.Players.LocalPlayer.PlayerGui.MainGui.Trading.Main.Countdown.Text == "0" and game.Players.LocalPlayer.PlayerGui.MainGui.Trading.Visible == true then
        wait(0.5)
        game.Players.LocalPlayer.PlayerGui.MainGui.SendMessage:Fire("DUPPING...", true)
        wait(2)
        game.Players.LocalPlayer:Kick("Rollbacked!")
    end
    wait()
end
